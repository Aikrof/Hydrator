{
    "name": "aikrof/hydrator",
    "type": "library",
    "description": "Convert objects to arrays and back",
    "keywords": [
        "hydrator",
        "mapper",
        "extract data from object to array",
        "hydrate data from array to object"
    ],
    "homepage": "https://github.com/Aikrof/Hydrator",
    "authors": [
        {
            "name": "Denys",
            "email": "AikrofStark@gmail.com",
            "homepage": "https://github.com/Aikrof"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
    	"php": ">=7.2.0",
        "phpdocumentor/reflection-docblock": "5.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
    	"psr-4": {
	        "Aikrof\\Hydrator\\": "src/"
	    }
    },
    "autoload-dev": {
    	"psr-4": {
	        "Aikrof\\Hydrator\\Tests\\": "tests/"
	    }
    },
    "config": {
        "secure-http": false
    }
}
